q(function() Tl(0.15,1,0.15,function(i) for ii,vv in pairs{'Top','Bottom'} do v[vv].Transparency=i*1 end end) v:Remove() end)
v.CFrame=tcf
HitBox(v,v3(lBlade,0.5,cm),cn(0,0,0),Dealt(),0.2)
wait(0.1)
Speed[1]=1
ReturnStance(1,1)
Anim='H'
end
end)
