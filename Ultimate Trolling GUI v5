Anim='HE' OH:Heat(OH.Cost[Anim],true)
Speed[1]=0
BGFace=true
qAni(la,-0.6,0.45,-0.7,-90,50, 0,2,ns*1.5,1)
 Ani(ra, 1.5,0.55,0,-90, 0,90,2,ns*1.5,1)
local c1=han.CFrame*cn(0,lBlade/2+0.5,0).p
