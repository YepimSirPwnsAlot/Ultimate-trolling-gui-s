JohnDoe.BackgroundColor3 = Color3.fromRGB(235, 190, 77)
JohnDoe.Position = UDim2.new(0.0093457941, 0, 0.0329205021, 0)
JohnDoe.Size = UDim2.new(0, 419, 0, 36)
JohnDoe.Font = Enum.Font.SourceSansBold
